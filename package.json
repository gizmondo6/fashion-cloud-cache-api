    "eslint:fix": "eslint --fix src/**/*.{js,ts}",
    "prettier": "prettier --check \"src/**/*.{js,ts}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,ts}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gizmondo6/fashion-cloud-cache-api.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gizmondo6/fashion-cloud-cache-api/issues"
  },
  "homepage": "https://github.com/gizmondo6/fashion-cloud-cache-api#readme",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.0.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.6",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "src/**/*": "npm run lint"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0",
    "winston": "^3.3.3"
  }
