{
  "name": "fashion-cloud-cache-api",
  "version": "1.0.0",
  "description": "Fashion Cloud technical challenge",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:dev": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "test": "jest",
    "prepare": "husky install",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint": "eslint src/**/*.{js,ts}",
    "eslint:fix": "eslint --fix src/**/*.{js,ts}",
    "prettier": "prettier --check \"src/**/*.{js,ts}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,ts}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gizmondo6/fashion-cloud-cache-api.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gizmondo6/fashion-cloud-cache-api/issues"
  },
  "homepage": "https://github.com/gizmondo6/fashion-cloud-cache-api#readme",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "src/**/*": "npm run lint"
  }
}
